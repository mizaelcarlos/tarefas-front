AA,MAAA;AACA,SAJA;AAMA,OARA,MAQA;AACA,QAAA,MAAA,CAAA,SAAA,CAAA,KAAA,MAAA,EAAA,KAAA,MAAA,CAAA,EAAA,EAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,UAAA,MAAA,CAAA,MAAA,GAAA,EAAA;AACA,UAAA,KAAA,CAAA,gCAAA,CAAA;;AACA,UAAA,MAAA,CAAA,MAAA;AACA,SAJA;AAMA;AAGA,KAlCA;AAoCA,IAAA,MApCA,kBAoCA,MApCA,EAoCA;AACA,WAAA,MAAA,GAAA,MAAA;AACA,KAtCA;AAwCA,IAAA,OAxCA,mBAwCA,MAxCA,EAwCA;AAAA;;AAEA,UAAA,OAAA,CAAA,6BAAA,CAAA,EAAA;AACA,QAAA,MAAA,CAAA,MAAA,CAAA,MAAA,EAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,UAAA,MAAA,CAAA,MAAA;AACA,SAFA;AAGA;AAEA;AAhDA;AArBA,CAAA","sourcesContent":["<template>\r\n  <div id=\"app\">\r\n\r\n    <nav>\r\n      <div class=\"nav-wrapper blue darken-1\">\r\n        <a href=\"#\" class=\"brand-logo center\">Consulta Tarefas</a>\r\n      </div>\r\n    </nav>\r\n\r\n    <div class=\"container\">\r\n      \r\n\r\n      <form @submit.prevent=\"salvar\">\r\n\r\n          <label>Titulo</label>\r\n          <input type=\"text\" placeholder=\"Titulo\" v-model=\"tarefa.titulo\">\r\n          <label>Descrição</label>\r\n          <input type=\"text\" placeholder=\"Descrição\" v-model=\"tarefa.descricao\">\r\n          <label>Feita</label>\r\n          <input type=\"text\" placeholder=\"Tarefa concluída?\" v-model=\"tarefa.feita\">\r\n          <label>Tipo</label>\r\n          <input type=\"number\" placeholder=\"Tipo\" v-model=\"tarefa.tipo_id\">\r\n\r\n          <button class=\"waves-effect waves-light btn-small\">Salvar<i class=\"material-icons left\">save</i></button>\r\n\r\n      </form>\r\n\r\n      <table>\r\n\r\n        <thead>\r\n\r\n          <tr>\r\n            <th>Titulo</th>\r\n            <th>Descrição</th>\r\n            <th>Concluída?</th>\r\n            <th>Tipo</th>\r\n            <th>OPÇÕES</th>\r\n          </tr>\r\n\r\n        </thead>\r\n\r\n        <tbody>\r\n\r\n          <tr v-for=\"tarefa of tarefas\" :key=\"tarefa.id\">\r\n\r\n            <td>{{tarefa.titulo}}</td>\r\n            <td>{{tarefa.descricao}}</td>\r\n            <td v-if= \"tarefa.feita == 0\">Não</td>\r\n            <td v-else>Sim</td>\r\n            <td>{{tipo['nome']}}</td>\r\n            <td>\r\n              <button @click=\"editar(tarefa)\" class=\"waves-effect btn-small blue darken-1\"><i class=\"material-icons\">create</i></button>\r\n              <button @click=\"remover(tarefa)\" class=\"waves-effect btn-small red darken-1\"><i class=\"material-icons\">delete_sweep</i></button>\r\n            </td>\r\n\r\n          </tr>\r\n\r\n        </tbody>\r\n      \r\n      </table>\r\n\r\n    </div>\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n\r\nimport Tarefa from '.././services/tarefas'\r\n\r\nexport default {\r\n\r\n  data(){\r\n\r\n    return {\r\n      tarefa: {\r\n        id: '',\r\n        titulo: '',\r\n        descricao: '',\r\n        feita: '',\r\n        tipo_id: ''\r\n      },\r\n      tarefas:[],\r\n      tipo: []\r\n    }\r\n  },\r\n  mounted(){\r\n    this.listar(),\r\n    this.obterTipoTarefa()\r\n  },\r\n\r\n  methods:{\r\n    \r\n    listar(){\r\n      Tarefa.listar().then(resposta => {\r\n        this.tarefas = resposta.data\r\n    })\r\n    },\r\n\r\n    obterTipoTarefa(){\r\n      Tarefa.obterTipo(this.tarefa.tipo_id).then(resposta => {\r\n        this.tipo = resposta.data\r\n    })\r\n    },\r\n\r\n    salvar(){\r\n      \r\n      if(!this.tarefa.id){\r\n        \r\n        Tarefa.guardar(this.tarefa).then(resposta => {\r\n        this.tarefa = {}\r\n        alert('Tarefa cadastrada com sucesso.')\r\n        this.listar()\r\n        })\r\n\r\n      }else{\r\n        Tarefa.atualizar(this.tarefa,this.tarefa.id).then(resposta => {\r\n        this.tarefa = {}\r\n        alert('Tarefa atualizado com sucesso.')\r\n        this.listar()\r\n        })\r\n\r\n      }\r\n\r\n      \r\n    },\r\n\r\n    editar(tarefa){\r\n      this.tarefa = tarefa\r\n    },\r\n\r\n    remover(tarefa){\r\n\r\n      if(confirm('Deseja remover essa tarefa?')){\r\n        Tarefa.apagar(tarefa).then(resposta => {\r\n        this.listar()\r\n      })\r\n      }\r\n      \r\n    }\r\n  }\r\n}\r\n\r\n</script>\r\n\r\n\r\n<style>\r\n\r\n</style>\r\n"],"sourceRoot":"src/components"}]}